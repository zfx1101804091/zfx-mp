<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zfx.demo.mapper.OneQMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zfx.demo.entity.OneQ">
        <id column="id" property="id" />
        <result column="name_q" property="nameQ" />
        <result column="name_y" property="nameY" />
        <result column="value_q" property="valueQ" />
        <result column="value_y" property="valueY" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name_q, name_y, value_q, value_y
    </sql>

</mapper>

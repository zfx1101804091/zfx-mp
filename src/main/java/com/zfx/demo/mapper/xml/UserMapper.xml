<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zfx.demo.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zfx.demo.entity.User">
        <id column="id" property="id" />
        <result column="role_id" property="roleId" />
        <result column="login_name" property="loginName" />
        <result column="name" property="name" />
        <result column="password" property="password" />
        <result column="state" property="state" />
        <result column="phone" property="phone" />
        <result column="code" property="code" />
        <result column="operator_id" property="operatorId" />
        <result column="editflag" property="editflag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, role_id, login_name, name, password, state, phone, code, operator_id, editflag
    </sql>

</mapper>
